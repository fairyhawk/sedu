            SELECT
            <include refid="Course_NS.COU_COURSE_TBL_columns"/>
            FROM COU_COURSE_TBL WHERE COU_COURSE_TBL.STATUS  != 2 
            <isNotEqual property="subjectId" compareValue="0" prepend="and" >
